{
  "name": "react-apexsankey",
  "version": "1.0.0",
  "description": "React wrapper for ApexSankey - A JavaScript library to create Sankey diagrams",
  "type": "module",
  "main": "./dist/react-apexsankey.cjs",
  "module": "./dist/react-apexsankey.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-apexsankey.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-apexsankey.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "sankey",
    "diagram",
    "chart",
    "apexsankey",
    "svg",
    "visualization",
    "data-visualization"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/apexcharts/react-apexsankey.git"
  },
  "bugs": {
    "url": "https://github.com/apexcharts/react-apexsankey/issues"
  },
  "homepage": "https://github.com/apexcharts/react-apexsankey#readme",
  "peerDependencies": {
    "@svgdotjs/svg.js": ">=3.0.0",
    "apexsankey": ">=1.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "@svgdotjs/svg.js": {
      "optional": true
    },
    "apexsankey": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  }
}